<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
	<link href="/css/placeholder.css" rel="stylesheet">

	<script src="/js/mapQuest.js"></script>

	<script src="https://api.mqcdn.com/sdk/mapquest-js/v1.3.2/mapquest.js"></script>
	<link type="text/css" rel="stylesheet" href="https://api.mqcdn.com/sdk/mapquest-js/v1.3.2/mapquest.css"/>


</head>

<body>
	<div class="back" id='PPH'>
		<i class="gg-close-o"></i>
	</div>

	<div class="content">

	      <h3>Make an Appointment</h3>

 	     <div id="mapQuest" class="profile">
 	       <h3>Location</h3>
 	       <div id="map" style="max-height: 400px;"></div>
 	     </div>

		 <form id="rsvpForm">
		   <div class="form-group">
			 <label for="rsvpEmail">Client Serial Number</label>
			 <input required type="name" class="form-control" id="rsvpEmail" aria-describedby="emailHelp" placeholder="Enter your serial number">
		   </div>
		   <div class="form-group">
			 <label for="rsvpExcited">Email</label>
			 <input required type="email" id="rsvpExcited" class="form-control" placeholder="">
		   </div>
		   <button type="submit" class="btn btn-primary">RSVP Now</button>
		 </form>

	      <br>

	</div>

	<script src="js/jquery-1.11.0.js"></script>
	<script src="js/bootstrap.js"></script>
	<script src="js/test.js"></script>

</body>
</html>
